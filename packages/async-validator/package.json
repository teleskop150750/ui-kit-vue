{
  "name": "@nado/async-validator",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "run-p type-check build-only dts",
    "build-only": "vite build",
    "dts": "tsc -p tsconfig.app.json --declaration --emitDeclarationOnly",
    "type-check": "tsc --noEmit -p ./tsconfig.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prettier": "prettier --check --ignore-unknown \"**/*\"",
    "prettier:fix": "prettier --write --ignore-unknown \"**/*\"",
    "eslint": "eslint \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\"",
    "eslint:fix": "eslint --fix \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\" --fix"
  },
  "devDependencies": {
    "@nado/eslint-config-ts": "workspace:*",
    "@nado/prettier-config": "workspace:*",
    "@nado/tsconfig": "workspace:*",
    "@types/node": "^18.15.3",
    "@vitest/coverage-c8": "^0.29.3",
    "@vitest/ui": "^0.29.3",
    "eslint": "^8.36.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.29.3"
  }
}
