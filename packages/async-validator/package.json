{
  "name": "@nado/async-validator",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p type-check clean build-only dts",
    "build-only": "vite build",
    "clean": "rimraf dist",
    "dts": "tsc -p tsconfig.app.json --outDir dist/types --declaration --emitDeclarationOnly --composite false",
    "type-check": "tsc --noEmit -p ./tsconfig.app.json --composite false",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prettier": "prettier --check --ignore-unknown \"**/*\"",
    "prettier:fix": "prettier --write --ignore-unknown \"**/*\"",
    "eslint": "eslint \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\"",
    "eslint:fix": "eslint --fix \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\" --fix"
  },
  "devDependencies": {
    "@nado/eslint-config-ts": "workspace:*",
    "@nado/prettier-config": "workspace:*",
    "@nado/tsconfig": "workspace:*",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitest/coverage-c8": "^0.29.3",
    "@vitest/ui": "^0.29.3",
    "eslint": "^8.34.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.1",
    "vite": "^4.2.0",
    "vitest": "^0.29.1"
  }
}
