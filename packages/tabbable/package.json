{
  "name": "@nado/tabbable",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "run-p type-check clean build-only dts",
    "build-only": "vite build",
    "clean": "rimraf dist",
    "dts": "tsc -p tsconfig.app.json --outDir dist/types --declaration --emitDeclarationOnly --composite false",
    "type-check": "tsc --noEmit -p ./tsconfig.app.json --composite false",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "ELECTRON_ENABLE_LOGGING=1 cypress run",
    "test:e2e:dev": "cypress open",
    "prettier": "prettier --check --ignore-unknown \"**/*\"",
    "prettier:fix": "prettier --write --ignore-unknown \"**/*\"",
    "eslint": "eslint \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\"",
    "eslint:fix": "eslint --fix \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\" --fix"
  },
  "devDependencies": {
    "@nado/eslint-config-ts": "workspace:*",
    "@nado/prettier-config": "workspace:*",
    "@nado/tsconfig": "workspace:*",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/dom": "^9.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.11",
    "@vitest/coverage-c8": "^0.29.8",
    "@vitest/ui": "^0.29.8",
    "cypress": "^12.9.0",
    "eslint-plugin-cypress": "^2.13.2",
    "jsdom": "^21.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.4.1",
    "start-server-and-test": "^2.0.0",
    "unbuild": "^1.2.0",
    "vitest": "^0.29.8"
  }
}
