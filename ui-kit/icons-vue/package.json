{
  "name": "@nado/ui-kit-icons-vue",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "generate": "tsx build-icons/generate.ts",
    "prettier": "prettier --check --ignore-unknown \"**/*\"",
    "prettier:fix": "prettier --write --ignore-unknown \"**/*\"",
    "eslint": "eslint \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\"",
    "eslint:fix": "eslint --fix \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\" --fix"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@nado/eslint-config-vue": "workspace:*",
    "@nado/prettier-config": "workspace:*",
    "@nado/ui-kit-icons-svg": "workspace:*",
    "@pnpm/find-workspace-dir": "^4.0.3",
    "@pnpm/find-workspace-packages": "^4.0.41",
    "@pnpm/logger": "^4.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.14.2",
    "@types/prettier": "^2.7.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.0",
    "camelcase": "^7.0.0",
    "chalk": "^5.1.0",
    "consola": "^2.15.3",
    "eslint": "^8.36.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  }
}
