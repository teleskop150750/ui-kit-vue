{
  "name": "@nado/ui-kit-components",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "main": "./src/components/index.ts",
  "module": "./src/components/index.ts",
  "types": "./src/components/index.ts",
  "scripts": {
    "type-check": "vue-tsc --noEmit -p ./tsconfig.app.json --composite false",
    "test": "pnpm run test:unit",
    "test:unit": "vitest",
    "test:unit.ui": "vitest --ui",
    "test:unit.run": "vitest run",
    "test:unit.coverage": "vitest run --coverage",
    "prettier": "prettier --check --ignore-unknown \"**/*\"",
    "prettier:fix": "prettier --write --ignore-unknown \"**/*\"",
    "eslint": "eslint \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\"",
    "eslint:fix": "eslint --fix \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\" --fix",
    "stylelint": "stylelint \"**/*.{css,vue}\"",
    "stylelint:fix": "stylelint --fix \"**/*.{css,vue}\""
  },
  "dependencies": {
    "@nado/async-validator": "workspace:*",
    "@nado/focus-trap": "workspace:*",
    "@nado/tabbable": "workspace:*",
    "@nado/ui-kit-constants": "workspace:*",
    "@nado/ui-kit-hooks": "workspace:*",
    "@nado/ui-kit-icons-vue": "workspace:*",
    "@nado/ui-kit-locale": "workspace:*",
    "@nado/ui-kit-theme": "workspace:*",
    "@nado/ui-kit-utils": "workspace:*",
    "@popperjs/core": "^2.11.7",
    "@types/lodash-es": "^4.17.7",
    "@vueuse/core": "^9.13.0",
    "lodash-es": "^4.17.21",
    "memoize-one": "^6.0.0"
  },
  "devDependencies": {
    "@nado/eslint-config-vue": "workspace:*",
    "@nado/prettier-config": "workspace:*",
    "@nado/stylelint-config-css": "workspace:*",
    "@nado/tsconfig": "workspace:*",
    "@nado/ui-kit-test-utils": "workspace:*",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.29.8",
    "@vue/test-utils": "^2.3.2",
    "jsdom": "^21.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.4.1",
    "stylelint": "^15.4.0",
    "type-fest": "^3.8.0",
    "vitest": "^0.29.8",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.2.0"
  }
}
