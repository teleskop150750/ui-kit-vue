{
  "name": "@nado/ui-kit-theme",
  "type": "module",
  "private": true,
  "license": "MIT",
  "style": "./dist/index.css",
  "exports": {
    "./styles": "./dist/index.css",
    "./components/all": "./dist/components/index.css",
    "./components/*": "./dist/components/*.css",
    "./fonts": "./dist/fonts.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "gulp",
    "dev": "gulp watch",
    "prettier": "prettier --check --ignore-unknown \"**/*\"",
    "prettier:fix": "prettier --write --ignore-unknown \"**/*\"",
    "eslint": "eslint \"**/*.{js,jsx,cjs,mjs,json,vue}\"",
    "eslint:fix": "eslint --fix \"**/*.{js,jsx,cjs,mjs,json,vue}\" --fix",
    "stylelint": "stylelint \"**/*.{css,vue}\"",
    "stylelint:fix": "stylelint --fix \"**/*.{css,vue}\""
  },
  "devDependencies": {
    "@nado/eslint-config-vue": "workspace:*",
    "@nado/prettier-config": "workspace:*",
    "@nado/stylelint-config-css": "workspace:*",
    "@rushstack/eslint-patch": "^1.2.0",
    "del": "5.1.0",
    "eslint": "^8.36.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-csso": "^6.0.1",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.0",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0"
  }
}
