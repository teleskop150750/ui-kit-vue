{
  "name": "@nado/ui-kit-vue",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build && pnpm run build-types",
    "build-types": "vue-tsc -p tsconfig.app.json --declaration --emitDeclarationOnly",
    "type-check": "vue-tsc --noEmit -p tsconfig.node.json --composite false",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prettier": "prettier --check --ignore-unknown \"**/*\"",
    "prettier:fix": "prettier --write --ignore-unknown \"**/*\"",
    "eslint": "eslint \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\"",
    "eslint:fix": "eslint --fix \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\" --fix",
    "stylelint": "stylelint \"**/*.{css,vue}\"",
    "stylelint:fix": "stylelint --fix \"**/*.{css,vue}\""
  },
  "dependencies": {
    "@nado/async-validator": "workspace:*",
    "@nado/focus-trap": "workspace:*",
    "@nado/tabbable": "workspace:*",
    "@nado/ui-kit-icons-vue": "workspace:*",
    "@popperjs/core": "^2.11.6",
    "@vue/shared": "^3.2.47",
    "@vueuse/core": "^9.13.0",
    "lodash-es": "^4.17.21",
    "memoize-one": "^6.0.0",
    "normalize-wheel-es": "^1.2.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@nado/eslint-config-vue": "workspace:*",
    "@nado/prettier-config": "workspace:*",
    "@nado/stylelint-config-css": "workspace:*",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.29.3",
    "@vue/test-utils": "^2.3.0",
    "eslint": "^8.36.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  }
}
