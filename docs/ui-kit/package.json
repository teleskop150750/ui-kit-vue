{
  "name": "@nado/ui-kit-vue-docs",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "histoire dev --config --host",
    "build": "run-p type-check histoire build --config",
    "preview": "histoire preview --config",
    "build-only": "histoire build --config",
    "prettier": "prettier --check --ignore-unknown \"**/*\"",
    "prettier:fix": "prettier --write --ignore-unknown \"**/*\"",
    "eslint": "eslint \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\"",
    "eslint:fix": "eslint --fix \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\" --fix",
    "stylelint": "stylelint \"**/*.{css,vue}\"",
    "stylelint:fix": "stylelint --fix \"**/*.{css,vue}\""
  },
  "dependencies": {
    "@nado/ui-kit-icons-vue": "workspace:*",
    "@nado/ui-kit-theme": "workspace:*",
    "@nado/ui-kit-vue": "workspace:*",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.15.8",
    "@nado/eslint-config-vue": "workspace:*",
    "@nado/prettier-config": "workspace:*",
    "@nado/stylelint-config-css": "workspace:*",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "eslint": "^8.34.0",
    "histoire": "^0.15.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  }
}
